package bot;

public class DmojQueue {//a queue that stores dmoj api call times
	private final int SIZE=100;//the maximum queue size, will not be exceeded due to dmoj rate limit of 90/minute
	private int start,end;
	private long[] queue=new long[SIZE];
	public DmojQueue() {
		start=0; end=-1;
	}
	public void push(long newVal) {
		end++;
		if(end==SIZE) end=0;
		queue[end]=newVal;
	}
	public long pop() {
		long val=queue[start];
		start++;
		if(start==SIZE) start=0;
		return val;
	}
	public long front() {
		return queue[start];
	}
	public int size() {
		return (end-start+1+SIZE)%SIZE;
	}
}
