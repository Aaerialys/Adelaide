package bot;

import java.util.HashMap;
import java.util.TreeSet;

public class GlobalProblem extends Problem{
	TreeSet<Integer> qualityVotes = new TreeSet<Integer>(),difficultyVotes = new TreeSet<Integer>();
	HashMap<Long,String> reviews = new HashMap<Long,String>();
	public GlobalProblem(String link) {
		super(link);
	}
	public void addQualityVote(int newVote) {
		qualityVotes.add(newVote);
	}
	public void addDifficultyVote(int newDifficulty) {
		difficultyVotes.add(newDifficulty);
	}
	
}
