package bot;

import java.util.ArrayList;
import java.util.HashMap;

public class GlobalProblem extends Problem{
	ArrayList<Integer> qualityVotes = new ArrayList<Integer>(),difficultyVotes = new ArrayList<Integer>();
	HashMap<Long,String> reviews = new HashMap<Long,String>();
	public GlobalProblem(String link) {
		super(link);
	}
	public void addQualityVote(int newVote) {
		qualityVotes.add(newVote);
		for(int i=qualityVotes.size()-1;i>0;i--) if(qualityVotes.get(i)<qualityVotes.get(i-1)) x=x^y^(y=x);
	}
	public void addDifficultyVote(int newDifficulty) {
		difficultyVotes.add(newDifficulty);
	}
	public int getQuality() {
		return qualityVotes.
	}
	
}
